ame="TierUpper">														<xs:annotation>															<xs:documentation>Interest Rate Upper Tier Limit. 0 = No Maximum Limit</xs:documentation>														</xs:annotation>														<xs:simpleType>															<xs:restriction base="xs:nonNegativeInteger">																<xs:minInclusive value="0"/>															</xs:restriction>														</xs:simpleType>													</xs:element>													<xs:element name="GrossRate">														<xs:annotation>															<xs:documentation>Gross Interest Rate</xs:documentation>														</xs:annotation>														<xs:simpleType>															<xs:restriction base="Decimal5_3">																<xs:minInclusive value="-2"/>																<xs:maxInclusive value="40"/>															</xs:restriction>														</xs:simpleType>													</xs:element>													<xs:element name="GrossRateExcludingBonus">														<xs:annotation>															<xs:documentation>Gross Interest Rate (excluding any introductory bonuses)</xs:documentation>														</xs:annotation>														<xs:simpleType>															<xs:restriction base="Decimal5_3">																<xs:minInclusive value="-2"/>																<xs:maxInclusive value="40"/>															</xs:restriction>														</xs:simpleType>													</xs:element>													<xs:element name="GrossAER">														<xs:annotation>															<xs:documentation>Gross AER</xs:documentation>														</xs:annotation>														<xs:simpleType>															<xs:restriction base="Decimal5_3">																<xs:minInclusive value="-2"/>																<xs:maxInclusive value="15"/>															</xs:restriction>														</xs:simpleType>													</xs:element>													<xs:element name="GrossAERExcludingBonus">														<xs:annotation>															<xs:documentation>Gross AER (excluding any introductory bonuses)</xs:documentation>														</xs:annotation>														<xs:simpleType>															<xs:restriction base="Decimal5_3">																<xs:minInclusive value="-2"/>																<xs:maxInclusive value="40"/>															</xs:restriction>														</xs:simpleType>													</xs:element>													<xs:element name="IntPaidOnType">														<xs:annotation>															<xs:documentation>Part of balance the interest is paid on (Full balance or Portion of balance)</xs:documentation>														</xs:annotation>														<xs:simpleType>															<xs:restriction base="NonNullString">																<xs:maxLength value="10"/>																<xs:enumeration value="Full"/>																<xs:enumeration value="Portion"/>															</xs:restriction>														</xs:simpleType>													</xs:element>												</xs:sequence>											</xs:complexType>										</xs:element>									</xs:sequence>									<xs:sequence>										<!-- Escaltor Bonds -->										<xs:element name="Escalator" minOccurs="0" maxOccurs="1">											<xs:annotation>												<xs:documentation>Escalator Bonds</xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:sequence>													<xs:element name="YearEndAnniversaryTick" type="Tick">														<xs:annotation>															<xs:documentation>Escalator Year End (either anniversary or day of year) - Anniversary of account opening</xs:documentation>														</xs:annotation>													</xs:element>													<xs:element name="YearEndMonth">														<xs:annotation>															<xs:documentation>Escalator Year End (used in conjunction with YearEndDay) - Month component (0 = N/A)</xs:documentation>														</xs:annotation>														<xs:simpleType>															<xs:restriction base="xs:nonNegativeInteger">																<xs:minInclusive value="0"/>																<xs:maxInclusive value="12"/>															</xs:restriction>														</xs:simpleType>													</xs:element>													<xs:element name="YearEndDay">														<xs:annotation>															<xs:documentation>Escalator Year End (used in conjunction with YearEndMonth) - Day of month component (0 = N/A)</xs:documentation>														</xs:annotation>														<xs:simpleType>															<xs:restriction base="xs:nonNegativeInteger">																<xs:minInclusive value="0"/>																<xs:maxInclusive value="31"/>															</xs:restriction>														</xs:simpleType>													</xs:element>												</xs:sequence>											</xs:complexType>										</xs:element>										<xs:element name="EscalatorRate" minOccurs="0" maxOccurs="unbounded">											<xs:annotation>												<xs:documentation>Escalator Bond Rates</xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:sequence>													<xs:element name="TierLower">														<xs:annotation>															<xs:documentation>Interest Rate Lower Tier Limit</xs:documentation>														</xs:annotation>														<xs:simpleType>															<xs:restriction base="xs:nonNegativeInteger">																<xs:minInclusive value="1"/>															</xs:restriction>														</xs:simpleType>													</xs:element>													<xs:element name="TierUpper">														<xs:annotation>															<xs:documentation>Interest Rate Upper Tier Limit. 0 = No Maximum Limit</xs:documentation>														</xs:annotation>														<xs:simpleType>															<xs:restriction base="xs:nonNegativeInteger">																<xs:minInclusive value="0"/>															</xs:restriction>														</xs:simpleType>													</xs:element>													<xs:element name="PeriodYrs">														<xs:annotation>															<xs:documentation>																Bond Period (Year component).																																Example Periods (Yearly escalations):-																		PeriodYrs	PeriodMths		Period		