ement name="FundingDetail" nillable="true">								<xs:annotation>									<xs:documentation>Funding requirements (if applicable). This information is only available for accounts with the AccountStatus "Open to New Business"</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="100"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="IncomeDetail" nillable="true">								<xs:annotation>									<xs:documentation>Minimum income requirements (if applicable). This information is only available for accounts with the AccountStatus "Open to New Business"</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="100"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="LaunchDate" type="NullableDateTime">								<xs:annotation>									<xs:documentation>Product launch date</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="WefDate" type="xs:dateTime">								<xs:annotation>									<xs:documentation>With effect from date</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="RateNote">								<xs:annotation>									<xs:documentation>Rate details for accounts which do not have published interest rates or are constantly changing</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="150"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="RateLinkNote">								<xs:annotation>									<xs:documentation>Details for when interest rates are linked to inflations, bank base rate etc</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">									