uence>								</xs:complexType>							</xs:element>							<xs:element name="TransferOption" minOccurs="0" maxOccurs="unbounded">								<xs:annotation>									<xs:documentation>Transfer Options. This information is only available for accounts with the AccountStatus "Open to New Business"</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="TransferInDetail">											<xs:annotation>												<xs:documentation>Details of where transfers can be made from</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="NonNullString">													<xs:maxLength value="40"/>													<xs:enumeration value="Cash APS ISA"/>													<xs:enumeration value="Cash Junior ISA"/>													<xs:enumeration value="Child Trust Fund"/>													<xs:enumeration value="Cash ISA"/>                          <xs:enumeration value="Cash LISA"/>													<xs:enumeration value="Help to Buy ISA"/>													<xs:enumeration value="Innovative Finance ISA"/>													<xs:enumeration value="Stocks and Shares APS ISA"/>													<xs:enumeration value="Stocks and Shares ISA"/>													<xs:enumeration value="Stocks and Shares JISA"/>                          <xs:enumeration value="Stocks and Shares LISA"/>													<xs:enumeration value="TESSA Only ISA"/>												</xs:restriction>											</xs:simpleType>										</xs:element>									</xs:sequence>								</xs:complexType>							</xs:element>							<xs:element name="IntroBonus" minOccurs="0" maxOccurs="3">								<xs:annotation>									<xs:documentation>Introductory Bonus Information</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="IntroBonusOrder">											<xs:annotation>												<xs:documentation>Order of Intro bonuses when an account has more than one</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:nonNegativeInteger">													<xs:minInclusive value="1"/>													<xs:maxInclusive value="5"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="IntroBonusRate">											<xs:annotation>												<xs:documentation>Introductory Bonus Rate</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="Decimal6_3">													<xs:minInclusive value="0"/>													<xs:maxInclusive value="10"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="IntroBonusVariableTick" type="Tick">											<xs:annotation>												<xs:documentation>Introductory bonus is variable</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="IntroBonusPeriod">											<xs:annotation>												<xs:documentation>Introductory Bonus Period (in months)</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:nonNegativeInteger">													<xs:minInclusive value="0"/>													<xs:maxInclusive value="60"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="IntroBonusDate" type="NullableDateTime">											<xs:annotation>												<xs:documentation>Introductory Bonus Period Until Date</xs:documentation>											</xs:annotation>										</xs:element>									</xs:sequence>								</xs:complexType>							</xs:element>							<xs:element name="DepositorProtection" minOccurs="0" maxOccurs="1">                <xs:annotation>                  <xs:documentation>Depositor Protection Information</xs:documentation>                </xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="OwningGroup">											<xs:annotation>												<xs:documentation>Owning Group</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="NonNullString">													<xs:maxLength value="200"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="LicencedBy">											<xs:annotation>												<xs:documentation>Deposit Taking (Banking) License</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="200"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="BanksInGroup">											<xs:annotation>												<xs:documentation>Other banks operating under the same Banking License</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="200"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="CompensationScheme">											<xs:annotation>												<xs:documentation>                          Compensation Schemes. Combination of:-                               Cyprus                              Dutch                              Gibraltar                              Guernsey                              Isle of Man                              Jersey                              UK                              Governor Money                              Swedish                        </xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="150"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="CompensationSchemeDescription">											<xs:annotation>												<xs:documentation>Details of the Compensation Scheme</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="2500"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="Notes">											<xs:annotation>												<xs:documentation>Further information</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="200"/>												</xs:restriction>											</xs:simpleType>										</xs:element>									</xs:sequence>								</xs:complexType>							</xs:element>						</xs:sequence>            <xs:attribute name="uniqueid" use="required">              <xs:simpleType>                <xs:restriction base="xs:string">                  <xs:maxLength value="50"/>                </xs:restriction>              </xs:simpleType>            </xs:attribute>            <xs:attribute name="MoneyfactsID" use="required">              <xs:annotation>                <xs:documentation>Moneyfacts Unique Product ID</xs:documentation>              </xs:annotation>              <xs:simpleType>                <xs:restriction base="xs:string">                  <xs:maxLength value="50"/>                </xs:restriction>              </xs:simpleType>            </xs:attribute>					</xs:complexType>				</xs:element>			</xs:sequence>			<xs:attribute name="date" type="xs:dateTime" use="required"/>		</xs:complexType>	</xs:element>	<!-- DataTypes -->	<xs:simpleType name="NonNullString">		<xs:annotation>			<xs:documentation>Data type used to verify that a string type contains a value</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:string">			<xs:minLength value="1"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="Decimal5_2">		<xs:annotation>			<xs:documentation>This type represents a number with 3 digits before and 2 digits after the decimal place.</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:decimal">			<xs:totalDigits value="5"/>			<xs:fractionDigits value="2"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="Decimal5_3">		<xs:annotation>			<xs:documentation>This type represents a number with 2 digits before and 3 digits after the decimal place.</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:decimal">			<xs:totalDigits value="5"/>			<xs:fractionDigits value="3"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="Decimal6_3">		<xs:annotation>			<xs:documentation>This type represents a number with 3 digits before and 3 digits after the decimal place.</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:decimal">			<xs:totalDigits value="6"/>			<xs:fractionDigits value="3"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="Decimal6_4">		<xs:annotation>			<xs:documentation>This type represents a number with 2 digits before and 4 digits after the decimal place.</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:decimal">			<xs:totalDigits value="6"/>			<xs:fractionDigits value="4"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="Tick">		<xs:annotation>			<xs:documentation>Represents a simple binary checkbox 'true' or 'false'</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:string">			<xs:enumeration value="true"/>			<xs:enumeration value="false"/>		</xs:restriction>	</xs:simpleType>	<xs:simpleType name="NullableDateTime">		<xs:annotation>			<xs:documentation>Type to allow a date time or a null value</xs:documentation>		</xs:annotation>		<xs:union>			<xs:simpleType>				<xs:restriction base="xs:string">					<xs:enumeration value=""/>				</xs:restriction>			</xs:simpleType>			<xs:simpleType>				<xs:restriction base="xs:dateTime"/>			</xs:simpleType>		</xs:union>	</xs:simpleType>	<xs:simpleType name="NullableNonNegativeInteger">		<xs:annotation>			<xs:documentation>Non negative integer which also allows null values</xs:documentation>		</xs:annotation>		<xs:union memberTypes="xs:nonNegativeInteger">			<xs:simpleType>				<xs:restriction base="xs:string">					<xs:enumeration value=""/>				</xs:restriction>			</xs:simpleType>		</xs:union>	</xs:simpleType></xs:schema>