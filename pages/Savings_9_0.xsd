<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">	<xs:element name="Savings">		<xs:complexType>			<xs:sequence>				<xs:element name="Account" minOccurs="1" maxOccurs="unbounded">					<xs:complexType>						<xs:sequence>							<xs:element name="HistNo" type="xs:nonNegativeInteger">								<xs:annotation>									<xs:documentation>Moneyfacts Account ID</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="AccountType">								<xs:annotation>									<xs:documentation>Account Type</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="NonNullString">										<xs:maxLength value="50"/>										<xs:enumeration value="Business Fixed"/>										<xs:enumeration value="Business Offshore Fixed"/>										<xs:enumeration value="Business Offshore Variable"/>										<xs:enumeration value="Business Variable"/>										<xs:enumeration value="Cash ISA"/>										<xs:enumeration value="Cash Junior ISA"/>										<xs:enumeration value="Child Trust Fund"/>										<xs:enumeration value="Childrens"/>										<xs:enumeration value="Escalator"/>										<xs:enumeration value="Escalator ISA"/>										<xs:enumeration value="Escalator Junior ISA"/>										<xs:enumeration value="Fixed"/>										<xs:enumeration value="Growth"/>										<xs:enumeration value="Help to Buy ISA"/>                    <xs:enumeration value="Lifetime ISA"/>										<xs:enumeration value="Offshore Current"/>										<xs:enumeration value="Offshore Escalator"/>										<xs:enumeration value="Offshore Fixed"/>										<xs:enumeration value="Offshore Growth"/>										<xs:enumeration value="Offshore Variable"/>										<xs:enumeration value="Regular Savings"/>										<xs:enumeration value="Regular Savings Childrens"/>										<xs:enumeration value="Regular Savings ISA"/>										<xs:enumeration value="Regular Savings Junior ISA"/>										<xs:enumeration value="TESSA Only ISA"/>										<xs:enumeration value="Variable"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="CurrencyCodeAlpha">								<xs:annotation>									<xs:documentation>Currency Code alphanumeric</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="NonNullString">                    <xs:maxLength value="3"/>										<xs:enumeration value="EUR"/>										<xs:enumeration value="GBP"/>										<xs:enumeration value="USD"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="CurrencyDisplaySymbol">								<xs:annotation>									<xs:documentation>Currency Display Symbol</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="NonNullString">										<xs:maxLength value="10"/>										<xs:enumeration value="€         "/>										<xs:enumeration value="£         "/>										<xs:enumeration value="US$       "/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="CurrencyCodeNumeric">								<xs:annotation>									<xs:documentation>Currency ISO Code</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="NonNullString">										<xs:maxLength value="3"/>										<xs:enumeration value="978"/>										<xs:enumeration value="826"/>										<xs:enumeration value="840"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="AccountStatus">								<xs:annotation>									<xs:documentation>Account status</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="NonNullString">										<xs:maxLength value="60"/>										<xs:enumeration value="Closed to New Business - Accepting contributions"/>										<xs:enumeration value="Closed to New Business - Not accepting contributions"/>										<xs:enumeration value="Open to New Business"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="CompNo" type="xs:nonNegativeInteger">								<xs:annotation>									<xs:documentation>Moneyfacts unique customer number</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="Company">								<xs:annotation>									<xs:documentation>Company name</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="NonNullString">										<xs:maxLength value="80"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="IssuerCompNo" type="xs:nonNegativeInteger">								<xs:annotation>									<xs:documentation>Company number of account issuer</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="IssuerCompany">								<xs:annotation>									<xs:documentation>Company name of the account issuer</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="NonNullString">										<xs:maxLength value="80"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="OperatedByCompNo" type="NullableNonNegativeInteger">								<xs:annotation>									<xs:documentation>Company number the account is operated by (if not holding company)</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="OperatedByCompany">								<xs:annotation>									<xs:documentation>Company the account is operated by (if not holding company)</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="80"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="Account">								<xs:annotation>									<xs:documentation>Account name</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="NonNullString">										<xs:maxLength value="50"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="AccountOption">								<xs:annotation>									<xs:documentation>Account option name</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="60"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="Telephone">								<xs:annotation>									<xs:documentation>Account telephone number</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="50"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="WebSite">								<xs:annotation>									<xs:documentation>Account Web Site</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="100"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="MiscContact">								<xs:annotation>									<xs:documentation>Miscellaneous contact details</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="100"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="MinAgeYrs">								<xs:annotation>									<xs:documentation>Minimum age allowed of account holder - Year component. 0 = No minimum [Personal accounts]</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:union>										<xs:simpleType>											<xs:restriction base="xs:nonNegativeInteger">												<xs:minInclusive value="0"/>												<xs:maxInclusive value="99"/>											</xs:restriction>										</xs:simpleType>										<xs:simpleType>											<xs:restriction base="xs:string">												<xs:enumeration value=""/>											</xs:restriction>										</xs:simpleType>									</xs:union>								</xs:simpleType>							</xs:element>							<xs:element name="MinAgeMths">								<xs:annotation>									<xs:documentation>Minimum age allowed of account holder - Month component [Personal accounts]</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:union>										<xs:simpleType>											<xs:restriction base="xs:nonNegativeInteger">												<xs:minInclusive value="0"/>												<xs:maxInclusive value="11"/>											</xs:restriction>										</xs:simpleType>										<xs:simpleType>											<xs:restriction base="xs:string">												<xs:enumeration value=""/>											</xs:restriction>										</xs:simpleType>									</xs:union>								</xs:simpleType>							</xs:element>							<xs:element name="MaxAgeYrs">								<xs:annotation>									<xs:documentation>Maximum age allowed of account holder - Year component. 0 = No maximum [Personal accounts]</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:union>										<xs:simpleType>											<xs:restriction base="xs:nonNegativeInteger">												<xs:minInclusive value="0"/>												<xs:maxInclusive value="115"/>											</xs:restriction>										</xs:simpleType>										<xs:simpleType>											<xs:restriction base="xs:string">												<xs:enumeration value=""/>											</xs:restriction>										</xs:simpleType>									</xs:union>								</xs:simpleType>							</xs:element>							<xs:element name="AgeNote">								<xs:annotation>									<xs:documentation>Notes relating to age [Personal accounts]</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="1000"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="MinAge">								<xs:annotation>									<xs:documentation>Minimum age of account holder (text version of MinAgeYrs and MinAgeMths) [Personal accounts]</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="20"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="MaxAge">								<xs:annotation>									<xs:documentation>Maximum age of account holder (text version of MaxAgeYrs) [Personal accounts]</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="20"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="MinTurnover">								<xs:annotation>									<xs:documentation>Minimum turnover required [Business Accounts]</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:union>										<xs:simpleType>											<xs:restriction base="xs:nonNegativeInteger">												<xs:minInclusive value="0"/>												<xs:maxInclusive value="10000000"/>											</xs:restriction>										</xs:simpleType>										<xs:simpleType>											<xs:restriction base="xs:string">												<xs:enumeration value=""/>											</xs:restriction>										</xs:simpleType>									</xs:union>								</xs:simpleType>							</xs:element>							<xs:element name="MaxTurnover">								<xs:annotation>									<xs:documentation>Maximum turnover allowed [Business Accounts]</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:union>										<xs:simpleType>											<xs:restriction base="xs:nonNegativeInteger">												<xs:minInclusive value="0"/>												<xs:maxInclusive value="100000000"/>											</xs:restriction>										</xs:simpleType>										<xs:simpleType>											<xs:restriction base="xs:string">												<xs:enumeration value=""/>											</xs:restriction>										</xs:simpleType>									</xs:union>								</xs:simpleType>							</xs:element>							<xs:element name="MinOperateBal" type="xs:nonNegativeInteger">								<xs:annotation>									<xs:documentation>The minimum operating balance. 0 = No minimum</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="MinInv" type="xs:nonNegativeInteger">								<xs:annotation>									<xs:documentation>The minimum initial investment.</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="MinInvCurrencyCodeAlpha">								<xs:annotation>									<xs:documentation>Minimum Investment - ISO Currency 3 letter alpha currency code</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="NonNullString">										<xs:maxLength value="3"/>										<xs:enumeration value="EUR"/>										<xs:enumeration value="GBP"/>										<xs:enumeration value="USD"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="MinInvCurrencyDisplaySymbol">								<xs:annotation>									<xs:documentation>Minimum Investment - Currency symbol</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="NonNullString">										<xs:maxLength value="10"/>										<xs:enumeration value="€         "/>										<xs:enumeration value="£         "/>										<xs:enumeration value="US$       "/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="MinInvCurrencyCodeNumeric">								<xs:annotation>									<xs:documentation>Minimum Investment - ISO Currency numeric currency code</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="NonNullString">										<xs:maxLength value="3"/>										<xs:enumeration value="978"/>										<xs:enumeration value="826"/>										<xs:enumeration value="840"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="LocalMinInv" type="xs:nonNegativeInteger">								<xs:annotation>									<xs:documentation>The minimum investment accepted from inside the local area where this is different to MinInv. 0 = Same as MinInv</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="MaxInv" type="xs:nonNegativeInteger">								<xs:annotation>									<xs:documentation>The maximum investment allowed in the account. 0 = No maximum</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="MinAddition" type="xs:nonNegativeInteger">								<xs:annotation>									<xs:documentation>Minimum addition allowed. 0 = No minimum</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="MinWithdrawal" type="xs:nonNegativeInteger">								<xs:annotation>									<xs:documentation>Minimum withdrawal allowed. 0 = No minimum</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="FurtherAdditionTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates if further additions are allowed into the account</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="FurtherAdditionNote">								<xs:annotation>									<xs:documentation>Details on further additions</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="200"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="WithdrawalTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates that withdrawals are allowed</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="EarlyAccessTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates if early access is allowed - Applies to notice accounts only (default to '0' for Instants)</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="CurrentAccountAccessTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates if the account can be accessed via a nominated current account</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="AccessNote">								<xs:annotation>									<xs:documentation>Notes on withdrawals and early access restrictions.</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="250"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="AccountFee">								<xs:annotation>									<xs:documentation>Account fee</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="Decimal5_2">										<xs:minInclusive value="0"/>										<xs:maxInclusive value="500"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="AccountFeeFrq">								<xs:annotation>									<xs:documentation>Account fee frequency</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="20"/>										<xs:enumeration value="Mly"/>										<xs:enumeration value="Qly"/>										<xs:enumeration value="Yly"/>										<xs:enumeration value=""/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="AccountFeeCurrencyCodeAlpha">								<xs:annotation>									<xs:documentation>Account Fee - ISO Currency 3 letter alpha currency code</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="3"/>										<xs:enumeration value="EUR"/>										<xs:enumeration value="GBP"/>										<xs:enumeration value="USD"/>										<xs:enumeration value=""/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="AccountFeeCurrencyDisplaySymbol">								<xs:annotation>									<xs:documentation>Account Fee - Currency symbol</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="10"/>										<xs:enumeration value="€         "/>										<xs:enumeration value="£         "/>										<xs:enumeration value="US$       "/>										<xs:enumeration value=""/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="AccountFeeCurrencyCodeNumeric">								<xs:annotation>									<xs:documentation>Account Fee - ISO Currency numeric currency code</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="3"/>										<xs:enumeration value="978"/>										<xs:enumeration value="826"/>										<xs:enumeration value="840"/>										<xs:enumeration value=""/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="AccountFeeNote">								<xs:annotation>									<xs:documentation>Account Fee - Further information</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:maxLength value="50"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="OpenBranchTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates that the account can be opened at a branch</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="OpenInternetTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates that the account can be opened via the internet</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="OpenPostTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates that the account can be opened by post</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="OpenTelephoneTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates that the account can be opened by telephone</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="OpenMobileAppTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates that the account can be opened using a mobile app</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="BranchTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates that the account can be operated through a branch</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="TeleTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates that the account can be operated by telephone</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="PostTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates that the account can be operated by post</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="INetTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates that the account can be operated via the internet</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="MobileAppTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates that the account can be operated using a mobile app</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="PassBookTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates if a pass book is available</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="CashCardTick" type="Tick">								<xs:annotation>									<xs:documentation>Indicates if a cash card is available</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="CashCardAvailAge">								<xs:annotation>									<xs:documentation>Childrens Accounts - Age at which the Cash Card becomes available</xs:documentation>								</xs:annotation>								<xs:simpleType>									<xs:restriction base="xs:nonNegativeInteger">										<xs:minInclusive value="0"/>										<xs:maxInclusive value="21"/>									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element name="DebitCardTick" type="Tick" nillable="true">								<xs:annotation>									<xs:documentation>Indicates if a debit card is available. This information is only available for accounts with the AccountStatus "Open to New Business"</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="ChqBookTick" type="Tick" nillable="true">								<xs:annotation>									<xs:documentation>Indicates if a cheque book is available. This information is only available for accounts with the AccountStatus "Open to New Business"</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="DDTick" type="Tick" nillable="true">								<xs:annotation>									<xs:documentation>Indicates if direct debits are available. This information is only available for accounts with the AccountStatus "Open to New Business"</xs:documentation>								</xs:annotation>							</xs:element>							<xs:element name="SOTick" type="Tick" nillable="true">								<xs:annotation>									<xs:documentation>Indicates if standing orders are available. This information is only available for accounts with the Acc